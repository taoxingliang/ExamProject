<?xml version="1.0" encoding="UTF-8" ?>  
<!DOCTYPE mapper  
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">  
<mapper namespace="db.dao.ExamDao">  
    <select id="findExamById" parameterType="String" resultType="Exam">  
        select * from exam where id=#{id}  
    </select>
    <insert id="insertExamInDb" parameterType="Exam">  
        insert into exam (examId,
							region,
							questionType,
							questionNum,
							questionInfo)  
              values   (#{examId},
						#{region},
						#{questionType},
						#{questionNum},
						#{questionInfo})
    </insert>
</mapper>  